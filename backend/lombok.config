# Enable Lombok's configuration system
config.stopBubbling = true

# Configure Lombok to add @Generated annotations to generated methods
lombok.addLombokGeneratedAnnotation = true

# Enable @SuperBuilder support
lombok.anyConstructor.addConstructorProperties = true

# Configure delombok to handle @SuperBuilder with @Data
lombok.experimental.flagUsage = WARNING

# Enable @SuperBuilder to work with @Value
lombok.singular.useGuava = true

# Configure @SuperBuilder to be more lenient with generics
lombok.singular.auto = true
